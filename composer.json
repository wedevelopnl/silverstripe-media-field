{
    "name": "thewebmen/silverstripe-media-field",
    "description": "Silverstripe media field for a image or a video",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "form",
        "image",
        "video"
    ],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Michel van der Steege",
        "email": "michel@thewebmen.com"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "silverstripe/cms": "^4.0@dev",
        "silverstripe/vendor-plugin": "^1.0",
        "silverstripe/display-logic": "^2.0",
        "embed/embed": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "installer-name": "silverstripe-media-field",
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "TheWebmen\\MediaField\\": "src/"
        }
    }
}
